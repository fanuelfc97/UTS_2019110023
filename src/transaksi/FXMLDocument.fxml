<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="transaksi.FXMLDocumentController">
   <children>
      <MenuBar layoutX="14.0" layoutY="14.0">
         <menus>
            <Menu mnemonicParsing="false" text="Master">
               <items>
                  <MenuItem fx:id="masterkonsumen" mnemonicParsing="false" onAction="#masterkonsumenklik" text="Konsumen" />
                  <MenuItem fx:id="masterproduk" mnemonicParsing="false" onAction="#masterprodukklik" text="Produk" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Transaksi">
               <items>
                  <MenuItem fx:id="transaksipenjualan" mnemonicParsing="false" onAction="#transaksipenjualanklik" text="Penjualan" />
                  <MenuItem fx:id="transaksireturpenjualan" mnemonicParsing="false" onAction="#transaksireturpenjualanklik" text="Retur Penjualan" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Display">
               <items>
                  <MenuItem fx:id="displaykonsumen" mnemonicParsing="false" onAction="#klikdisplaykonsumen" text="Konsumen" />
                  <MenuItem fx:id="displayproduk" mnemonicParsing="false" onAction="#displayprodukklik" text="Produk" />
                  <MenuItem fx:id="displaypenjualan" mnemonicParsing="false" onAction="#displaypenjualanklik" text="Penjualan" />
                  <MenuItem fx:id="displayreturpenjualan" mnemonicParsing="false" onAction="#displayreturpenjualanklik" text="Retur Penjualan" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Keluar">
               <items>
                  <MenuItem fx:id="Keluar" mnemonicParsing="false" onAction="#klikkeluar" text="Keluar" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
   </children>
</AnchorPane>
